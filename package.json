{
  "name": "the-gospels-express",
  "version": "1.0.0",
  "description": "Express server with local data to share api with the gospels of Jesus Christ",
  "main": "server.js",
  "scripts": {
    "install": "npm install && cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "author": "Tina Bisgaard",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "axios": "^0.20.0",
    "nodemon": "^2.0.4"
  }
}
